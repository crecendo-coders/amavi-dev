    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug Front-End",
      "url": "http://localhost:2319", 
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:///./*": "${webRoot}/*"
      }
    },    
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Node.js",
      "program": "${workspaceFolder}/server/index.js", 
      "runtimeArgs": [
        "--inspect"
      ],
      "stopOnEntry": false,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
    
  ]
}